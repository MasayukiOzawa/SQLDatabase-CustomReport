  <DataSets>
    <DataSet Name="ProductInfo">
      <Query>
        <DataSourceName>LocalServer</DataSourceName>
        <CommandText>SELECT 
CONVERT(nvarchar, SERVERPROPERTY('ProductVersion')) 
+ N' ' + CONVERT(nvarchar,SERVERPROPERTY('ProductLevel')) 
+ N' (' + CONVERT(nvarchar,SERVERPROPERTY('Edition')) + N')' AS ProductInfo</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ProductInfo">
          <DataField>ProductInfo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ServerStartTime">
      <Query>
        <DataSourceName>LocalServer</DataSourceName>
        <CommandText>SELECT DATEADD(ms, AVG(-wait_time_ms), GETDATE()) AS approximate_restart_date
FROM sys.dm_os_wait_stats w
WHERE wait_type IN ('DIRTY_PAGE_POLL','HADR_FILESTREAM_IOMGR_IOCOMPLETION','LAZYWRITER_SLEEP','LOGMGR_QUEUE','REQUEST_FOR_DEADLOCK_SEARCH','XE_DISPATCHER_WAIT','XE_TIMER_EVENT')
-- http://www.brentozar.com/archive/2015/08/when-did-my-azure-sql-database-server-restart/</CommandText>
      </Query>
      <Fields>
        <Field Name="approximate_restart_date">
          <DataField>approximate_restart_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="QueryStoreBase">
      <Query>
        <DataSourceName>LocalServer</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SortOrder">
            <Value>=Parameters!SortOrder.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ResultCount">
            <Value>=Parameters!ResultCount.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT *
FROM(
	SELECT
	CASE @SortOrder
		WHEN 1 THEN ROW_NUMBER() OVER(ORDER BY SUM(rs.avg_duration) DESC) 
		WHEN 2 THEN ROW_NUMBER() OVER(ORDER BY SUM(rs.avg_cpu_time) DESC) 
		WHEN 3 THEN ROW_NUMBER() OVER(ORDER BY SUM(rs.avg_logical_io_reads) DESC) 
		WHEN 4 THEN ROW_NUMBER() OVER(ORDER BY SUM(rs.avg_logical_io_writes) DESC) 
		WHEN 5 THEN ROW_NUMBER() OVER(ORDER BY SUM(rs.avg_physical_io_reads) DESC) 
		WHEN 6 THEN ROW_NUMBER() OVER(ORDER BY SUM(rs.avg_rowcount) DESC) 
		WHEN 7 THEN ROW_NUMBER() OVER(ORDER BY SUM(rs.count_executions) DESC) 
		WHEN 8 THEN ROW_NUMBER() OVER(ORDER BY COUNT(distinct p.plan_id) DESC) 
	END AS Number,
    p.query_id query_id,
    qt.query_sql_text query_text,
    CONVERT(bigint, SUM(rs.avg_duration) / 1000) AS avg_duration_ms,
	CONVERT(bigint, SUM(rs.avg_cpu_time) / 1000)  AS avg_cpu_time_ms,
	CONVERT(bigint, SUM(rs.avg_logical_io_reads) / 1000) AS avg_logical_io_reads_ms,
	CONVERT(bigint, SUM(rs.avg_logical_io_writes) / 1000) AS avg_logical_io_writes_ms,
	CONVERT(bigint, SUM(rs.avg_physical_io_reads) / 1000) AS avg_physical_io_reads_ms,
	CONVERT(bigint, SUM(rs.avg_rowcount) / 1000) AS avg_rowcount,
    SUM(rs.count_executions) count_executions,
    COUNT(distinct p.plan_id) num_plans,
	'0x' + CONVERT(nvarchar(255), q.query_hash,2) AS query_hash
FROM 
	sys.query_store_runtime_stats rs
    JOIN sys.query_store_plan p ON p.plan_id = rs.plan_id
    JOIN sys.query_store_query q ON q.query_id = p.query_id
    JOIN sys.query_store_query_text qt ON q.query_text_id = qt.query_text_id
WHERE
	rs.last_execution_time BETWEEN DATEADD(dd, -14, GETDATE()) AND GETDATE()
GROUP BY 
	p.query_id
	, qt.query_sql_text
	, q.query_hash
) AS QueryStore
WHERE
	Number &lt;= @ResultCount 
ORDER BY
	Number</CommandText>
      </Query>
      <Fields>
        <Field Name="Number">
          <DataField>Number</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="query_id">
          <DataField>query_id</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="query_text">
          <DataField>query_text</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="avg_duration_ms">
          <DataField>avg_duration_ms</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="avg_cpu_time_ms">
          <DataField>avg_cpu_time_ms</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="avg_logical_io_reads_ms">
          <DataField>avg_logical_io_reads_ms</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="avg_logical_io_writes_ms">
          <DataField>avg_logical_io_writes_ms</DataField>
