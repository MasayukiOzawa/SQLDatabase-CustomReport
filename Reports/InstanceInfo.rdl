  <DataSets>
    <DataSet Name="WaitStats">
      <Query>
        <DataSourceName>LocalServer</DataSourceName>
        <CommandText>SELECT *
	FROM sys.dm_os_wait_stats 
	WHERE wait_type IN('SOS_SCHEDULER_YIELD', 'CXPACKET', 'CMEMTHREAD', 'WRITELOG', 'ASYNC_IO_COMPLETION', 'ASYNC_NETWORK_IO', 'IO_COMPLETION')
UNION ALL
SELECT
	'LCK_M_xx',
	SUM(waiting_tasks_count),
	SUM(wait_time_ms),
	MAX(max_wait_time_ms),
	SUM(signal_wait_time_ms)
FROM
	sys.dm_os_wait_stats 
WHERE wait_type LIKE 'LCK_M_%'
UNION ALL
SELECT
	'PAGEIOLATCH_xx',
	SUM(waiting_tasks_count),
	SUM(wait_time_ms),
	MAX(max_wait_time_ms),
	SUM(signal_wait_time_ms)
FROM
	sys.dm_os_wait_stats 
WHERE wait_type LIKE 'PAGEIOLATCH_%'
UNION ALL
SELECT
	'PAGELATCH_xx',
	SUM(waiting_tasks_count),
	SUM(wait_time_ms),
	MAX(max_wait_time_ms),
	SUM(signal_wait_time_ms)
FROM
	sys.dm_os_wait_stats 
WHERE wait_type LIKE 'PAGELATCH_%'
UNION ALL
SELECT
	'LATCH_xx',
	SUM(waiting_tasks_count),
	SUM(wait_time_ms),
	MAX(max_wait_time_ms),
	SUM(signal_wait_time_ms)
FROM
	sys.dm_os_wait_stats 
WHERE wait_type LIKE 'LATCH_%'
UNION ALL
SELECT
	'BACKUPxx',
	SUM(waiting_tasks_count),
	SUM(wait_time_ms),
	MAX(max_wait_time_ms),
	SUM(signal_wait_time_ms)
FROM
	sys.dm_os_wait_stats 
WHERE wait_type LIKE 'BACKUP%'
ORDER BY wait_time_ms DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="wait_type">
          <DataField>wait_type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="waiting_tasks_count">
          <DataField>waiting_tasks_count</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="wait_time_ms">
          <DataField>wait_time_ms</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="max_wait_time_ms">
          <DataField>max_wait_time_ms</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="signal_wait_time_ms">
          <DataField>signal_wait_time_ms</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DatabaseInfo">
      <Query>
        <DataSourceName>LocalServer</DataSourceName>
        <CommandText>select name from sys.databases where name &lt;&gt; 'master'</CommandText>
      </Query>
      <Fields>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ServerStartTime">
      <Query>
        <DataSourceName>LocalServer</DataSourceName>
        <CommandText>SELECT DATEADD(ms, AVG(-wait_time_ms), GETDATE()) AS approximate_restart_date
FROM sys.dm_os_wait_stats w
WHERE wait_type IN ('DIRTY_PAGE_POLL','HADR_FILESTREAM_IOMGR_IOCOMPLETION','LAZYWRITER_SLEEP','LOGMGR_QUEUE','REQUEST_FOR_DEADLOCK_SEARCH','XE_DISPATCHER_WAIT','XE_TIMER_EVENT')
-- http://www.brentozar.com/archive/2015/08/when-did-my-azure-sql-database-server-restart/</CommandText>
      </Query>
      <Fields>
        <Field Name="approximate_restart_date">
          <DataField>approximate_restart_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ProductInfo">
      <Query>
        <DataSourceName>LocalServer</DataSourceName>
        <CommandText>SELECT 
CONVERT(nvarchar, SERVERPROPERTY('ProductVersion')) 
+ N' ' + CONVERT(nvarchar,SERVERPROPERTY('ProductLevel')) 
+ N' (' + CONVERT(nvarchar,SERVERPROPERTY('Edition')) + N')' AS ProductInfo</CommandText>
      </Query>
      <Fields>
        <Field Name="ProductInfo">
          <DataField>ProductInfo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
